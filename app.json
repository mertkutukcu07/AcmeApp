{
  "expo": {
    "name": "acme-app",
    "slug": "acme-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/root/icon.png",
    "userInterfaceStyle": "automatic",

    "splash": {
      "image": "./assets/root/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#475BD8"
    },

    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.acme.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/root/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.acme.app"
    },
    "web": {
      "favicon": "./assets/root/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-localization"]
  }
}
